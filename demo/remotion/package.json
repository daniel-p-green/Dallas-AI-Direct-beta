{
  "name": "@dallas-ai-direct/demo-remotion",
  "private": true,
  "version": "0.0.2",
  "description": "Isolated Remotion workspace for Dallas AI Direct overview video assets.",
  "scripts": {
    "preview": "remotion studio src/index.js",
    "preview:overview": "remotion studio src/index.tsx",
    "render": "remotion render src/index.js DallasAIDirectFlow ../../public/demo/dallas-ai-direct-remotion-demo.mp4 --codec=h264 --pixel-format=yuv420p --concurrency=2 --crf=30",
    "still": "remotion still src/index.js DallasAIDirectFlow ../../public/demo/dallas-ai-direct-remotion-demo-poster.png --frame=120 --image-format=png",
    "check:artifacts": "node ./scripts/check-artifacts.mjs",
    "render:overview": "remotion render src/index.tsx DallasAIBetaOverview ../../public/demo/dallas-ai-direct-beta-overview.mp4 --codec=h264 --pixel-format=yuv420p --concurrency=2 --crf=22",
    "still:overview": "remotion still src/index.tsx DallasAIBetaOverview ../../public/demo/dallas-ai-direct-beta-overview-poster.png --frame=336 --image-format=png",
    "check:overview": "node ./scripts/check-overview-artifacts.mjs",
    "generate:overview": "npm run render:overview && npm run still:overview && npm run check:overview"
  },
  "dependencies": {
    "@geist-ui/icons": "^1.0.2",
    "@remotion/tailwind": "^4.0.422",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "remotion": "^4.0.306"
  },
  "devDependencies": {
    "@remotion/cli": "^4.0.422",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.10",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3"
  }
}
